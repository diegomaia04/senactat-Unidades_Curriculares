/**
 * Fundamentos do Java
 * Estruturas de Repetições
 */
package fundamentos;

/**
 * @author Fagner Geraldes Braga
 *
 */
public class EstruturasRepeticoes {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// Estrutura For
		System.out.println("Exemplo 1 - for");
		for (int i = 0; i < 11; i++) {
			System.out.println(i);
		}
		System.out.println("-------------------------------");
		System.out.println("Exemplo 2 - for");
		for (int j = 10; j > -1; j--) {
			System.out.println(j);
		}
		System.out.println("-------------------------------");
		System.out.println("Exemplo 2 - tabuada");
		int valor = 7;
		for (int x = 1; x < 11; x++) {
			System.out.println(valor + " X " + x + " = " + );
		}
		

	} // Fim do método main

} // Fim da classe