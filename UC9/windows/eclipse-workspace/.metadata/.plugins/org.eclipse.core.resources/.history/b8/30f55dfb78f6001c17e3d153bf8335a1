 * 
 */
package fundamentos;

/**
 * @author Fagner Geraldes Braga
 *
 */
public class OperadoresComparativos {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println("         Operadores Comparativos");
		System.out.println("--------------------------------------");
		System.out.println("|     Operador     |     Definição    ");
		System.out.println("|        >         |     Maior que    ");
		System.out.println("|        <         |     Menor que    ");
		System.out.println("|        >=        | Maior ou igual a ");
		System.out.println("|        <=        | Menor ou igual a ");
		System.out.println("|        ==        |       Igual a    ");
		System.out.println("|        !=        |     Diferente    ");
		

	}

}
